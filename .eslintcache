es } from 'react-jss'\nimport { Link } from 'react-router-dom'\n\n// External Components\nimport { Grid, Typography } from '@material-ui/core'\n\n// Style Components\nimport { D66ThemeType } from '../../../styles/Theme'\n\n// Asset imports\nimport logoSource from '../../../images/logo.svg'\n\n// Styles\nconst useStyles = createUseStyles((theme:D66ThemeType) => ({\n  logoContainer: {\n    margin: '80px 0',\n    '& img': {\n      width: '100%',\n    },\n  },\n}))\n\nconst Logo:FunctionComponent = () => {\n  const classes = useStyles()\n\n  return (\n    <Grid container className={classes.logoContainer}>\n      <Grid item xs={12} md={6}>\n        <Grid item xs={6} md={4}>\n          <Link to=\"/\">\n            <img src={logoSource} alt=\"d66 logo\" />\n          </Link>\n        </Grid>\n      </Grid>\n    </Grid>\n  )\n}\n\nexport default Logo\n","/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Molecules/Disclaimer/index.tsx",[],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Organisms/Login/index.tsx",[],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/util/helpers.ts",[],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Organisms/Main/index.tsx",["107","108","109","110","111","112","113","114","115"],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/util/generateValidateInputIcon.tsx",[],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Goals/AddGoal/index.tsx",["116","117"],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Goals/GoalItem/index.tsx",["118","119","120","121"],"// Dependencies\nimport React, { FunctionComponent, useState } from 'react'\nimport { createUseStyles } from 'react-jss'\nimport moment from 'moment'\n\n// External components\nimport {\n  Grid,\n  Typography,\n  IconButton,\n} from '@material-ui/core'\n\n// Icons\nimport CloseIcon from '@material-ui/icons/Close'\n\n// Internal components\nimport DeleteGoal from '../DeleteGoal'\nimport Entry from '../../Entry'\n\n// Data Access\nimport { Goal, Entry as EntryType } from '../../../models'\n\n// Style Components\nimport { D66ThemeType } from '../../../styles/Theme'\n\n// Util\nimport { generateStringFromDate } from '../../../util/helpers'\n\n// Styles\nconst useStyles = createUseStyles((theme: D66ThemeType) => ({\n  goalItemContainer: {\n    marginBottom: `${theme.spacing.base}px`,\n  },\n  goalItem: {\n    padding: `${theme.spacing.base * 2}px`,\n    border: `1px solid ${theme.colors.red}`,\n    position: 'relative',\n  },\n  title: {\n    paddingBottom: `${theme.spacing.base / 2}px`,\n  },\n  dateCreated: {\n    paddingBottom: `${theme.spacing.base / 2}px`,\n  },\n  description: {\n    paddingBottom: `${theme.spacing.base / 4}px`,\n  },\n  closeButtonContainer: {\n    position: 'absolute',\n    top: theme.spacing.base * 2,\n    right: theme.spacing.base * 2,\n  },\n}))\n\ninterface GoalItemProps extends Goal {\n  fetchGoals?: any,\n}\n\nconst GoalItem:FunctionComponent<GoalItemProps> = ({\n  id,\n  title,\n  description,\n  dateCreated,\n  entries = new Array<EntryType>(),\n  fetchGoals = null,\n}) => {\n  const classes = useStyles()\n  const [isDeleting, setIsDeleting] = useState(false)\n  const [error, setError] = useState(null)\n\n  const handleDeleteGoal = async () => {\n    try {\n      const response = await fetch('/api/goals', {\n        method: 'DELETE',\n        headers: { 'Content-type': 'application/json' },\n        body: JSON.stringify({ id }),\n      })\n      const jsonResponse = await response.json()\n      if (!response.ok) {\n        setError(jsonResponse.message)\n        return\n      }\n      // happy path\n      // fetch all goals on parent component\n      fetchGoals()\n    } catch (err) {\n      console.error('Delete goal failed with error: ', err)\n      throw err\n    }\n  }\n\n  return (\n    <Grid container justify=\"center\" className={classes.goalItemContainer}>\n      <Grid item xs={12} sm={12} md={8} lg={6} className={classes.goalItem}>\n        { isDeleting\n          ? (\n            <DeleteGoal\n              setIsDeleting={setIsDeleting}\n              handleDeleteGoal={handleDeleteGoal}\n              goalTitle={title}\n            />\n          )\n          : (\n            <>\n              <Typography variant=\"h4\" component=\"h2\" className={classes.title}>\n                { title }\n              </Typography>\n              <Typography variant=\"caption\" component=\"p\" className={classes.dateCreated}>\n                created on\n                {' '}\n                { moment(dateCreated).format('ddd, MMM Do') }\n              </Typography>\n              <Typography variant=\"body1\" component=\"p\" className={classes.description}>\n                { description }\n              </Typography>\n              <Entry goalId={id} entries={entries} />\n              <div className={classes.closeButtonContainer}>\n                <IconButton\n                  color=\"primary\"\n                  aria-label=\"delete\"\n                  onClick={() => setIsDeleting(!isDeleting)}\n                >\n                  <CloseIcon />\n                </IconButton>\n              </div>\n            </>\n          )}\n      </Grid>\n    </Grid>\n  )\n}\n\nexport default GoalItem\n","/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Molecules/Tip/index.tsx",[],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Goals/DeleteGoal/index.tsx",["122","123"],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Molecules/AppBar/index.tsx",["124","125","126"],"/Users/vphilot/Desktop/web/juno-fullstack-masterclass/final-project/d66/src/components/Icons/index.tsx",[],{"ruleId":"127","severity":1,"message":"128","line":12,"column":3,"nodeTyp